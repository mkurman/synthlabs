{
  "name": "synthlabs-reasoning-generator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "bun:dev": "bun vite",
    "bun:build": "bun vite build",
    "bun:preview": "bun vite preview",
    "bun:compile": "bun vite build && bun build --compile --minify server.ts --outfile synthlabs",
